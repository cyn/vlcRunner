{
  "name": "vlc-runner",
  "description": "the app allows you to start streaming videos in vlc directly from your browser",
  "version": "0.0.2",
  "homepage": "./",
  "author": "cyn",
  "dependencies": {
    "express": "4.14.0",
    "peerflix": "0.36.0",
    "parse-torrent": "^5.4.0",
    "body-parser": "1.15.2",
    "numeral": "1.5.6",
    "torrent-stream": "1.0.3",
    "mime": "^1.2.11",
    "pump": "^1.0.1",
    "range-parser": "^1.0.0",
    "tree-kill": "1.1.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "build": {
    "appId": "cyn.first.app",
    "productName": "VLC runner",
    "mac": {
      "category": "public.app-category.business"
    }
  },
  "scripts": {
    "ui-build": "cd ui && react-scripts build",
    "ui-start": "cd ui && react-scripts start",
    "ui-eject": "cd ui && react-scripts eject",
    "pack": "npm run ui-build && build -m --dir",
    "dist": "npm run ui-build && build -m"
  },
  "devDependencies": {
    "react-devtools": "^2.0.12",
    "react-scripts": "0.9.5",
    "electron": "1.6.1",
    "electron-builder": "^15.6.2"
  }
}
